            <Font name="Roboto" size="12.0" />
         </font>
      </TextField>
      <Label maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="15.0" prefWidth="240.0" text="Room name" />
      <TextField fx:id="roomNameField" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="25.0" prefWidth="240.0" promptText="Enter a room name">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <font>
            <Font name="Roboto" size="12.0" />
         </font>
      </TextField>
      <Button fx:id="enterButton" mnemonicParsing="false" onAction="#enterRoom" prefWidth="240.0" text="Enter">
         <VBox.margin>
            <Insets />
         </VBox.margin>
