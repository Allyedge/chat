<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="10.0" styleClass="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.allyedge.HomeController">
   <children>
      <Label text="Chat App">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Label maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="15.0" prefWidth="240.0" text="Username" />
